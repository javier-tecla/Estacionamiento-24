Table ajustes {
    id int [pk, increment]
    nombre varchar
    descripcion text
    sucursal varchar
    direccion text
    telefonos varchar
    logo varchar
    logo_auto varchar
    divisa varchar
    correo varchar
    pagina_web varchar
    created_at timestamp
    updated_at timestamp
}

Table usuarios {
    id int [pk, increment]
    name varchar
    email varchar
    password varchar
    remember_token varchar

    nombres varchar
    apellidos varchar
    tipo_documento varchar
    numero_documento varchar
    celular varchar
    fecha_nacimiento date
    genero varchar
    direccion text
    foto varcharh
    contacto_nombre varchar
    contacto_telefono varchar
    contacto_parentesco varchar

    created_at timestamp
    updated_at timestamp
}